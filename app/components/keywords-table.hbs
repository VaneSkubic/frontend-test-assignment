<table id="data-table">
    <thead>
        <tr>
            <th>Query</th>
            <th>Position</th>
            <th>Search Volume</th>
            <th>Result Count</th>
            <th>Last Month Change</th>
            <th>Average Competitor Rank</th>
            <th>Best Competitor Rank</th>
